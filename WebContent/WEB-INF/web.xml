<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Flighttracker</display-name>
 
  	<servlet>
        <servlet-name>flighttracker</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
	<servlet-mapping>
    <servlet-name>flighttracker</servlet-name>
    <url-pattern>/</url-pattern>
    <url-pattern>/signup</url-pattern>
    <url-pattern>/login</url-pattern>
    <url-pattern>/invalid</url-pattern>
    <url-pattern>/exists</url-pattern>
    <url-pattern>/existsadmin</url-pattern>
    <url-pattern>/signupconf</url-pattern>
    <url-pattern>/loginconf</url-pattern>
    <url-pattern>/invalidlogin</url-pattern>
    <url-pattern>/logout</url-pattern>
    <url-pattern>/admintools</url-pattern>
    <url-pattern>/adminedit</url-pattern>
    <url-pattern>/adminadd</url-pattern>
    <url-pattern>/passmatch</url-pattern>
    <url-pattern>/reptools</url-pattern>
    <url-pattern>/addaircraft</url-pattern>
    <url-pattern>/addaircraftconf</url-pattern>
    <url-pattern>/aircraftexists</url-pattern>
    <url-pattern>/aircraftedit</url-pattern>
    <url-pattern>/aircraftdel</url-pattern>
    <url-pattern>/addairports</url-pattern>
    <url-pattern>/addairportsconf</url-pattern>
    <url-pattern>/airportexists</url-pattern>
    <url-pattern>/airportsedit</url-pattern>
    <url-pattern>/airportsdel</url-pattern>
    <url-pattern>/duplicate</url-pattern>
    <url-pattern>/flightsadd</url-pattern>
    <url-pattern>/flightsaddconf</url-pattern>
    <url-pattern>/flightsedit</url-pattern>
    <url-pattern>/flightseditconf</url-pattern>
    <url-pattern>/flightsdel</url-pattern>
    <url-pattern>/represerve</url-pattern>
    <url-pattern>/addres</url-pattern>
    <url-pattern>/resedit</url-pattern>
    <url-pattern>/reseditconf</url-pattern>
    <url-pattern>/addresconf</url-pattern>
    <url-pattern>/repwaitinglist</url-pattern>
    <url-pattern>/waitinglist</url-pattern>
    <url-pattern>/salesreport</url-pattern>
    <url-pattern>/reslist</url-pattern>
    <url-pattern>/revgen</url-pattern>
    <url-pattern>/customerrev</url-pattern>
    <url-pattern>/ticketssold</url-pattern>
    <url-pattern>/allflights</url-pattern>
	
  </servlet-mapping>
    
    
  
